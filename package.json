{
  "name": "ngx-property-editor-workspace",
  "version": "1.0.0",
  "license": "MIT",
  "description": "An Angular library containing simple input components and a property editor component which automatically builds a form for editing all properties of any object.",
  "keywords": [
    "angular",
    "bootstrap",
    "input"
  ],
  "author": "Heiner Walter",
  "repository": {
    "type": "git",
    "url": "https://github.com/heinerwalter/ngx-property-editor.git"
  },
  "scripts": {
    "ng": "ng",
    "link:demo": "npm link ./projects/ngx-property-editor",
    "start:demo": "ng serve demo",
    "build": "ng build ngx-property-editor && cd projects/ngx-property-editor && npm run build",
    "build:demo": "ng build demo --configuration production",
    "watch": "ng build ngx-property-editor --watch --configuration development",
    "test": "ng test ngx-property-editor --code-coverage",
    "test:demo": "ng test demo",
    "test-headless": "ng test ngx-property-editor --browsers=ChromeHeadless --watch=false",
    "test-headless:demo": "ng test demo --browsers=ChromeHeadless --watch=false --code-coverage",
    "lint": "ng lint ngx-property-editor",
    "publish": "cd dist/ngx-property-editor/ && npm publish",
    "increment-version": "node scripts/increment-version.js",
    "increment-version-and-commit": "npm run increment-version && npm i && git add package.json package-lock.json projects/ngx-property-editor/package.json && git commit -m \"Incremented package version.\" && git push",
    "pic": "npm run publish && npm run increment-version-and-commit"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@angular/animations": "^17.3.12",
    "@angular/common": "^17.3.12",
    "@angular/compiler": "^17.3.12",
    "@angular/core": "^17.3.12",
    "@angular/forms": "^17.3.12",
    "@angular/platform-browser": "^17.3.12",
    "@angular/platform-browser-dynamic": "^17.3.12",
    "@angular/router": "^17.3.12",
    "@cospired/i18n-iso-languages": "^4.2.0",
    "@fortawesome/angular-fontawesome": "^0.14.1",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@ng-bootstrap/ng-bootstrap": "^16.0.0",
    "@popperjs/core": "^2.11.8",
    "@sweetalert2/ngx-sweetalert2": "^12.4.0",
    "bootstrap": "^5.3.3",
    "chardet": "^2.1.0",
    "i18n-iso-countries": "^7.14.0",
    "ngx-property-editor": "^0.17.14",
    "ngx-toastr": "^19.0.0",
    "rxjs": "^7.8.2",
    "sweetalert2": "^11.14.5",
    "tslib": "^2.8.1",
    "zone.js": "^0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.12",
    "@angular-eslint/builder": "^17.5.3",
    "@angular-eslint/eslint-plugin": "^17.5.2",
    "@angular-eslint/eslint-plugin-template": "^17.5.2",
    "@angular-eslint/schematics": "^17.5.3",
    "@angular-eslint/template-parser": "^17.5.3",
    "@angular/cli": "^17.3.12",
    "@angular/compiler-cli": "^17.3.12",
    "@angular/localize": "^17.3.12",
    "@types/jasmine": "^4.6.5",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.57.1",
    "jasmine-core": "^4.6.1",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "ng-packagr": "^17.3.0",
    "typescript": "~5.2.2"
  }
}