{
  "name": "ngx-property-editor-workspace",
  "version": "1.0.0",
  "license": "MIT",
  "description": "An Angular library containing simple input components and a property editor component which automatically builds a form for editing all properties of any object.",
  "keywords": [
    "angular",
    "bootstrap",
    "input"
  ],
  "author": "Heiner Walter",
  "repository": {
    "type": "git",
    "url": "https://github.com/heinerwalter/ngx-property-editor.git"
  },
  "scripts": {
    "ng": "ng",
    "start:demo": "ng serve demo",
    "build": "ng build ngx-property-editor && cd projects/ngx-property-editor && npm run build",
    "build:demo": "ng build demo --configuration production",
    "watch": "ng build ngx-property-editor --watch --configuration development",
    "test": "ng test ngx-property-editor --code-coverage",
    "test-headless": "ng test ngx-property-editor --browsers=ChromeHeadless --watch=false --code-coverage",
    "lint": "ng lint ngx-property-editor",
    "publish": "cd dist/ngx-property-editor/ && npm publish"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@fortawesome/angular-fontawesome": "^0.13.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@ng-bootstrap/ng-bootstrap": "^15.1.2",
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.3.2",
    "ngx-property-editor": "^0.0.5",
    "rxjs": "^7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.0",
    "@angular-eslint/builder": "^16.2.0",
    "@angular-eslint/eslint-plugin": "^16.2.0",
    "@angular-eslint/eslint-plugin-template": "^16.2.0",
    "@angular-eslint/schematics": "^16.2.0",
    "@angular-eslint/template-parser": "^16.2.0",
    "@angular/cli": "^16.2.0",
    "@angular/compiler-cli": "^16.2.0",
    "@angular/localize": "^16.2.0",
    "@types/jasmine": "^4.3.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.49.0",
    "jasmine-core": "^4.6.0",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "ng-packagr": "^16.2.0",
    "typescript": "^5.1.3"
  }
}
